{
  "compilerOptions": {
    "jsx": "react",
    "allowJs": true,
    "target": "ESNext",
    "module": "ESNext",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules/**/*"]
}
